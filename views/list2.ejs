  <style>

  </style>
    <title>Welcome to <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="main.css" />
  </head>
  <body>
    <h1><%=shopName%> Shop</h1>
    <h2>Here are the foods we have information on:</h2>
    <ul class="nav">
      <form action="/updatefood" method="GET">
      
      <% availableFoods.forEach(function(food) { %>
          <br>Name:<input id="name" type="text" name="name" value="<%=food.name%>">
          <br>quantity:<input id="quantity" type="text" name="quantity" value="<%=food.quantity%>">
          <br>price of Measure:<input id="price" type="text" name="price" value="<%=food.price%>">
          <br> Carbohydrates:<input id="barcodeid" type="text" name="barcodeid" value="<%=food.barcodeid%>">

          <br> Added by User:<%=food.user%>
      <% }) %>
      
      <input type="submit" value="Update" >


      </form>
    </ul>
    </body>
</html>
